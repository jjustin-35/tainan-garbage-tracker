	},
	"type": "module",
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.16.8",
		"bits-ui": "^0.21.15",
		"clsx": "^2.1.1",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"vaul-svelte": "^0.3.2"
	}

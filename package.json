		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^9.6.0",
		"@types/google.maps": "^3.55.12",
		"autoprefixer": "^10.4.19",
		"cryptify": "^4.1.2",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
